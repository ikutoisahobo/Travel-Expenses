int   hotelExp()
{
 const int initial = 90;
 int exp;
 double totals;
 cout << "What were the expenses of your hotel?" << endl;
 cin >> exp;
 totals = exp - initial;
    if(exp > 90)
      cout << "You owe $" << totals << " for lodging" << endl;
    else
      cout << "The company paid for your expenses of $" << exp << endl;
  if(totals < 0)
  totals = 0;
}
